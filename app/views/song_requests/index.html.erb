<h1>Listing song_requests</h1>

<table>
  <tr>
    <th>Dj</th>
    <th>Song</th>
    <th>Singer</th>
    <th>Singer name</th>
    <th>Key</th>
    <th>Comments</th>
    <th>Archived</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @song_requests.each do |song_request| %>
  <tr>
    <td><%= song_request.dj_id %></td>
    <td><%= song_request.song_id %></td>
    <td><%= song_request.singer_id %></td>
    <td><%= song_request.singer_name %></td>
    <td><%= song_request.key %></td>
    <td><%= song_request.comments %></td>
    <td><%= song_request.archived %></td>
    <td><%= link_to 'Show', song_request %></td>
    <td><%= link_to 'Edit', edit_song_request_path(song_request) %></td>
    <td><%= link_to 'Destroy', song_request, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

